  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohMun/move-priority.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint"
  },
  "keywords": [
    "gesture",
    "priority",
    "observer",
    "swipe"
  ],
  "author": "Johannes Munari <johannesm@hotmail.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JohMun/move-priority/issues"
  },
  "homepage": "https://github.com/JohMun/move-priority#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@hydrant/eslint-config": "^2.1.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "extends": "@hydrant/eslint-config/vue",
    "rules": {}
  },
  "browserslist": [
    "ie >= 11",
    "last 2 Firefox versions",
    "last 5 Chrome versions",
    "last 2 iOS versions"
